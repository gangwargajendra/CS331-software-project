# Environment Configuration for Smart Traffic System
# Copy this file to .env and update with your settings

# Video Source
VIDEO_PATH=traffic_video.mp4
CAMERA_ID=0

# Model Configuration
MODEL_PATH=yolov8n.pt
CONFIDENCE_THRESHOLD=0.4

# API Configuration
API_HOST=127.0.0.1
API_PORT=5000
DEBUG=False

# Logging
LOG_FILE=traffic_logs.csv
LOG_ENABLED=True

# Traffic Signal Timing (seconds)
MIN_GREEN_TIME=5
MAX_GREEN_TIME=60
YELLOW_TIME=3
EMERGENCY_GREEN_TIME=30
